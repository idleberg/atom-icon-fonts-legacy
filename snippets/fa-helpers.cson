".text.html":
  "fa-li":
    description: "<li>"
    body: "<li><i class=\"fa-li fa $1\"></i>$2</li>$0"
    prefix: "fa-li"
  "fa-stack":
    body: '''
      <span class="fa-ul$1">
        $2
      </span>$0
    '''
    description: "<span>"
    prefix: "fa-stack"
  "fa-ul":
    body: '''
      <ul class="fa-ul">
        $1
      </ul>$0
    '''
    description: "<ul>"
    prefix: "fa-ul"
